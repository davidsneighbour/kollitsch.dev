{
  "$schema": "https://frontmatter.codes/config/data.types.schema.json",
  "id": "blogroll",
  "schema": {
    "file": "[[workspace]]/data/dnb/kollitsch/blogroll.json",
    "fileType": "json",
    "labelField": "name",
    "properties": {
      "description": {
        "multiline": true,
        "title": "Description",
        "type": "string"
      },
      "feed": {
        "title": "Feed",
        "type": "string"
      },
      "link": {
        "title": "Link",
        "type": "string"
      },
      "name": {
        "title": "Name",
        "type": "string"
      },
      "type": {
        "enum": [
          "blog",
          "podcast",
          "youtube"
        ],
        "title": "Type",
        "type": "string"
      }
    },
    "required": [
      "name",
      "link"
    ],
    "type": "object",
    "title": "Blogroll"
  }
}
