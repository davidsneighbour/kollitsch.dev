{
  "$schema": "https://frontmatter.codes/config/data.files.schema.json",
  "file": "[[workspace]]/src/content/blogroll.json",
  "id": "blogroll",
  "labelField": "name",
  "schema": {
    "properties": {
      "name": {
        "title": "Name",
        "type": "string"
      },
      "description": {
        "multiline": true,
        "title": "Description",
        "type": "string"
      },
      "url": {
        "title": "URL",
        "type": "string"
      },
      "rss": {
        "title": "RSS Feed URL",
        "type": "string"
      }
    },
    "required": ["name", "url"],
    "title": "Blogroll",
    "type": "object"
  },
  "title": "Blogroll"
}
