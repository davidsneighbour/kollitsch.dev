{
  "extends": ["html-validate:recommended", "html-validate:document"],
  "rules": {
    "no-style-tag": "error",
    "svg-focusable": "off",
    "no-unknown-elements": "off",
    "require-sri": "off",
    "no-trailing-whitespace": "off",
    "prefer-native-element": "off"
  },
  "elements": ["html5"],
  "root": true
}
