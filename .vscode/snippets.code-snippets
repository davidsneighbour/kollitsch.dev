    "scope": "markdown,frontmatter",
    "prefix": "dnb",
    "body": ["$CURRENT_YEAR-$CURRENT_MONTH-${CURRENT_DATE}T$CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND+07:00"],
    "description": "Insert date formatted for frontmatter"

  }
}
