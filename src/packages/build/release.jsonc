{
  "dependencies": {
    "@davidsneighbour/release-config": "2025.2.2"
  },
  "devDependencies": {},
  "scripts": {
    "release": "wireit"
  },
  "wireit": {
    "release": {
      "command": "commit-and-tag-version --sign -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" -- --no-verify && ./scripts/postrelease"
    }
  }
}
